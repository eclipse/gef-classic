				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.menu.foreground"
				value="COLOR_WIDGET_FOREGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.menu.foreground.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.menu.background.selected.label"
