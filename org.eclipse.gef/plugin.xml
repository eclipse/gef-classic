				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.button.darkest"
				value="COLOR_WIDGET_DARK_SHADOW">
			<description>%colorDefinition.org.eclipse.gef.color.button.darkest.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.list.background.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.list.background"
				value="COLOR_LIST_BACKGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.list.background.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.list.foreground.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.list.foreground"
				value="COLOR_LIST_FOREGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.list.foreground.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.line.foreground.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.line.foreground"
				value="COLOR_LIST_FOREGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.line.foreground.description</description>
		</colorDefinition>
		<colorDefinition
