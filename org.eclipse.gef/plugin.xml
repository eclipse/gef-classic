				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.menu.foreground.selected"
				value="COLOR_LIST_SELECTION_TEXT">
			<description>%colorDefinition.org.eclipse.gef.color.menu.foreground.selected.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.title.background.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.title.background"
				value="COLOR_TITLE_BACKGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.title.background.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.title.gradient.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.title.gradient"
				value="COLOR_TITLE_BACKGROUND_GRADIENT">
			<description>%colorDefinition.org.eclipse.gef.color.title.gradient.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.title.foreground.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.title.foreground"
				value="COLOR_TITLE_FOREGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.title.foreground.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.title.inactive.foreground.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.title.inactive.foreground"
				value="COLOR_TITLE_INACTIVE_FOREGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.title.inactive.foreground.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.title.inactive.background.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.title.inactive.background"
				value="COLOR_TITLE_INACTIVE_BACKGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.title.inactive.background.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.title.inactive.gradient.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.title.inactive.gradient"
				value="COLOR_TITLE_INACTIVE_BACKGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.title.inactive.gradient.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.tooltip.foreground.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.tooltip.foreground"
				value="COLOR_INFO_FOREGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.tooltip.foreground.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.tooltip.background.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.tooltip.background"
				value="COLOR_INFO_BACKGROUND">
			<description>%colorDefinition.org.eclipse.gef.color.tooltip.background.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.list.hover.background.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.list.hover.background"
				value="252, 228, 179">
			<description>%colorDefinition.org.eclipse.gef.color.list.hover.background.description</description>
		</colorDefinition>
		<colorDefinition
				label="%colorDefinition.org.eclipse.gef.color.list.selected.background.label"
				categoryId="org.eclipse.gef"
				id="org.eclipse.gef.color.list.selected.background"
				value="207, 227, 250">
			<description>%colorDefinition.org.eclipse.gef.color.list.selected.background.description</description>
		</colorDefinition>
	</extension>
	<extension
			point="org.eclipse.ui.views">
		<view
				name="%Palette.Label"
				icon="icons/palette_view.gif"
				category="org.eclipse.ui"
				class="org.eclipse.gef.ui.views.palette.PaletteView"
				id="org.eclipse.gef.ui.palette_view">
		</view>
	</extension>
	<extension
			point="org.eclipse.ui.perspectiveExtensions">
		<perspectiveExtension
				targetID="org.eclipse.ui.resourcePerspective">
			<view
					ratio="0.16"
					relative="org.eclipse.ui.editorss"
					visible="false"
					relationship="left"
					id="org.eclipse.gef.ui.palette_view">
			</view>
		</perspectiveExtension>
	</extension>

	<extension point="org.eclipse.ui.bindings">
		<key
			sequence="M1+="
			commandId="org.eclipse.gef.zoom_in"
			schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>
  		<key
        	commandId="org.eclipse.gef.zoom_in"
        	locale="de"
        	schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
        	sequence="M1++"/>
		<key
			sequence="M1+-"
			commandId="org.eclipse.gef.zoom_out"
			schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"/>

	</extension>
	<extension point="org.eclipse.core.runtime.adapters">
		<factory
				class="org.eclipse.gef.internal.PropertySourceAdapterFactory"
				adaptableType="org.eclipse.gef.editparts.AbstractEditPart">
			<adapter type="org.eclipse.ui.views.properties.IPropertySource"/>
		</factory>
	</extension>

</plugin>
