<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.gef.examples.logic"
   name="GEF Logic Editor Example"
   version="3.0.0"
   provider-name="Eclipse.org"
   class="org.eclipse.gef.examples.logicdesigner.LogicPlugin">

   <runtime>
      <library name="logic.jar"/>
   </runtime>
   <requires>
      <import plugin="org.eclipse.gef" version="3.0.0"/>
      <import plugin="org.eclipse.core.runtime" export="false"/>
      <import plugin="org.eclipse.core.resources" export="false"/>
      <import plugin="org.eclipse.ui.views" export="false"/>
      <import plugin="org.eclipse.ui.workbench" export="false"/>
      <import plugin="org.eclipse.ui.ide" export="false"/>
      <import plugin="org.eclipse.jface" export="false"/>
      <import plugin="org.eclipse.core.resources"/>
   </requires>


   <extension
         point="org.eclipse.ui.editors">
      <editor
            name="%Logic_example_graphical_editor_name"
            default="true"
            icon="logic.gif"
            extensions="logic"
            contributorClass="org.eclipse.gef.examples.logicdesigner.actions.LogicActionBarContributor"
            class="org.eclipse.gef.examples.logicdesigner.LogicEditor"
            id="Logic Graphical Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            name="%GEF_example_wizard_category_name"
            parentCategory="org.eclipse.ui.Examples"
            id="org.eclipse.gef.examples">
      </category>
      <wizard
            availableAsShortcut="true"
            name="%Logic_example_wizard_name"
            icon="logic.gif"
            category="org.eclipse.ui.Examples/org.eclipse.gef.examples"
            class="org.eclipse.gef.examples.logicdesigner.LogicCreationWizard"
            id="org.eclipse.gef.examples.logic.wizard.new.file">
         <description>
            %Logic_example_wizard_description
         </description>
         <selection
               class="org.eclipse.core.resources.IResource">
         </selection>
      </wizard>
   </extension>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            objectClass="org.eclipse.core.resources.IFile"
            nameFilter="*.logic"
            id="org.eclipse.gef.examples.logic">
         <action
               label="Print"
               class="org.eclipse.gef.examples.logicdesigner.actions.LogicPrintAction"
               menubarPath="additions"
               enablesFor="1"
               id="org.eclipse.gef.examples.logic.print">
         </action>
      </objectContribution>
   </extension>

</plugin>
